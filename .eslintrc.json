{
    "extends": [
      "standard"
    ],
    "plugins": [
      "svelte3"
    ],
    "parserOptions": {
      "ecmaVersion": 2019,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "globals": {},
    "rules": {
      "no-multiple-empty-lines": ["error", { "max": 2, "maxBOF": 2, "maxEOF": 0 }]
    },
    "overrides": [
      {
        "files": ["**/*.svelte"],
        "processor": "svelte3/svelte3",
        "rules": {
          "import/first": 0,
          "import/no-duplicates": 0,
          "import/no-mutable-exports": 0
        }
      }
    ]
}

